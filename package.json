{
  "name": "@slashid/agent-cdk",
  "version": "0.1.0",
  "description": "CDK constructs for deploying SlashID Agent",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "prepublishOnly": "npm run build && npm test",
    "example:synth": "npx cdk synth --app 'npx ts-node examples/bin/example-app.ts'",
    "example:deploy": "npx cdk deploy --app 'npx ts-node examples/bin/example-app.ts'",
    "example:destroy": "npx cdk destroy --app 'npx ts-node examples/bin/example-app.ts'"
  },
  "devDependencies": {
    "@types/jest": "^30",
    "@types/node": "^24.10.1",
    "aws-cdk": "2.1102.0",
    "jest": "^30",
    "ts-jest": "^29",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3"
  },
  "dependencies": {
    "@paulo_raca/cdk-skylight": "^0.0.6",
    "envfile": "^7.1.0",
    "yaml": "^2.8.2"
  },
  "keywords": [
    "cdk",
    "aws",
    "slashid",
    "identity",
    "agent"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/slashid/slashid-agent-cdk.git"
  }
}
